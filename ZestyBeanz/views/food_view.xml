<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<!--Form view-->
		<record id="view_food_food" model="ir.ui.view">
			<field name="name">model.food.food</field>
			<field name="model">food.food</field>
			<field name="arch" type="xml">
				<form string='Food'>
					<field name="name" />
				</form>
			</field>
		</record>

		<!-- list -->
		<record id="view_list_food_food" model="ir.ui.view">
			<field name="name">model.one.list</field>
			<field name="model">model.one</field>
			<field name="arch" type="xml">
				<list string='Food'>
					<field name="name" />
				</list>
			</field>
		</record>
		
		<!--action-->
		<record id="action_for_food" model="ir.actions.act_window">
			<field name="name">Food</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">food.food</field>
			<field name="view_mode">list,form</field>
			<field name="context">{}</field>
			<field name="help" type="html">
				<p>Create Food</p>
			</field>
		</record>

	</data>
</odoo>